@model MvcCv.Models.Entity.TblYeteneklerim
@{
    ViewBag.Title = "YetenekEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<br />
<h2  class="text-center my-4" style="background-color: #f3e5f5; color: #6a1b9a; ">Yeni Yetenek Ekleme Sayfası</h2>
<form method="post">
  
    @Html.Label("Yetenek")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">KAYDET</button>
</form>



